# encoding: utf-8
ActiveSupport::Inflector.inflections do |inflect|
	inflect.plural /^(ox)$/i, '\1\2en'
	inflect.singular /^(ox)en/i, '\1'
	inflect.irregular 'Πρόχειρο άρθρο', 'Πρόχειρα άρθρα'
	inflect.irregular 'Άρθρο προς υποβολή', 'Άρθρα προς υποβολή'
	inflect.irregular 'Μη αποδεκτό άρθρο', 'Μη αποδεκτά άρθρα'
	inflect.irregular 'Ολοκληρωμένο άρθρο', 'Ολοκληρωμένα άρθρα'
	inflect.irregular 'Προτεινόμενο άρθρο', 'Προτεινόμενα άρθρα'
	inflect.irregular 'Δημοσιευμένο άρθρο', 'Δημοσιευμένα άρθρα'
	inflect.irregular 'Φοιτητής', 'Φοιτητές'
	inflect.irregular 'Φοιτητής χρησιμοποίησε', 'Φοιτητές χρησιμοποίησαν'
	inflect.irregular 'Φοιτητής έχει', 'Φοιτητές έχουν'
end
